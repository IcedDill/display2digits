x = input('Whats yo numbo')
first = x[0]
second = x[1]
if first == "1":
    display1()
elif first = "2":
    display2()
elif first = "3":
    display3()
elif first = "4":
    display4()
elif first = "5":
    display5()
elif first = "6":
    display6()
elif first = "7":
